<configuration>
	<dbServerConnection>
		<HOST>localhost</HOST>
		<PORT>27017</PORT>
	</dbServerConnection>
	<dbServerSetup>
		<DB_NAME>healthbank</DB_NAME>
		<collection>
			<USER_COLLECTION_NAME>users</USER_COLLECTION_NAME>
			<INSTITUTES_COLLECTION_NAME>institutes</INSTITUTES_COLLECTION_NAME>
			<RECORDS_COLLECTION_NAME>records</RECORDS_COLLECTION_NAME>
			<SPACES_COLLECTION_NAME>spaces</SPACES_COLLECTION_NAME>
			<CIRCLES_COLLECTION_NAME>circles</CIRCLES_COLLECTION_NAME>
			<MESSAGES_COLLECTION_NAME>messages</MESSAGES_COLLECTION_NAME>
